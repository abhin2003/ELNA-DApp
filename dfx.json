{
  "canisters": {
    "backend": {
      "dependencies": ["wizard_details"],
      "type": "motoko",
      "main": "backend/Backend.mo"
    },
    "wizard_details": {
      "type": "motoko",
      "main": "wizard_details/Main.mo"
    },
    "elnaAi_backend": {
      "type": "rust",
      "candid": "../elnaAi/src/elnaAi_backend/elnaAi_backend.did",
      "package": "elnaAi_backend"
    },
    "frontend": {
      "dependencies": ["backend"],
      "type": "assets",
      "source": ["dist/"]
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env",
  "version": 2
}
