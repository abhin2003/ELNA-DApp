{
  "canisters": {
    "backend": {
      "type": "motoko",
      "main": "backend/Backend.mo"
    },
    "wizard_details": {
      "type": "motoko",
      "main": "wizard_details/Main.mo",
      "dependencies": ["backend"]
    },
    "frontend": {
      "dependencies": ["backend", "wizard_details", "elnaAi"],
      "type": "assets",
      "source": ["dist/"]
    },

    "elnaAi": {
      "type": "motoko",
      "main": "elnaAi/ChatAi.mo"
    }
  },

  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env",
  "version": 2
}
